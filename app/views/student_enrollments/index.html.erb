

<h1>Listado de Alumnos matriculados</h1>
<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Periodo</th>
      <th>Fecha Inicio</th>
      <th>Fecha Fin</th>
      <th>Horas</th>
      <th>opciones</th>
    </tr>
  </thead>
  <tbody>


        <% @student_enrollments.each do |f| %>
          <tr>
            <td><%= f.person.name%> </td>
            <td><%= f.person.lastName%> </td>
            <td><%= f.period.name%> </td>
            <td><%= f.period.date_init%> </td>
            <td><%= f.period.date_finish%> </td>
            <td><%= f.hours%> </td>
            <td>
              <%= link_to  student_enrollment_path(id:f.id) do %>
                  ver
                <% end %>
                <%= link_to edit_student_enrollment_path(f) do  %>
                  editar
                <% end %>
                <%= link_to  student_enrollment_path(f), method: :delete , data: { confirm: 'Â¿Estas Seguro?' } do %>
                  eliminar
                <% end %>
            </td>
          </tr>
        <%end%>


  </tbody>
</table>
<p><%= link_to new_student_enrollment_path, class:"white" do %>
      Nuevo
  <% end %>
</p>
