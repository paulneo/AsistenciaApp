<div class="">
  <%= form_for @person  do |f| %>
    <div class="">
      <%= f.label :Nombre %>
      <%= f.text_field :name %>
    </div>
    <div class="">
      <%= f.label :Apellido %>
      <%= f.text_field :lastName %>
    </div>
    <div class="">
      <%= f.label :Correo %>
      <%= f.text_field :email %>
    </div>
    <div class="">
      <%= f.label :Dni %>
      <%= f.text_field :dni %>
    </div>
    <div class="">
      <%= f.label :Celular  %>
      <%= f.text_field :cellphone %>
    </div>

      <%= f.submit "Enviar" ,class:"" %>

  <% end %>


</div>
